    "from scipy.stats import norm\n",
    "from numpy import power\n",
    "\n",
    "p1 = 0.90\n",
    "p2=0.95\n",
    "\n",
    "sigma=5/(norm.ppf(p2)-norm.ppf(p1))\n",
    "sigma_squared=power(sigma,2)\n",
    "mu=89-sigma*norm.ppf(0.90)\n",
    "\n",
    "print('sigma squared:',sigma_squared)\n",
    "print('mu:',mu)"
   ]
  }
