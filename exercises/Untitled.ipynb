    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "F=lambda x:np.where(0<x and x<100,x/200,np.where(x<0 ,0,1)) \n",
    "x=np.linspace(-1000,1000).tolist()\n",
    "plt.plot(x,[F(i) for i in x],label='F(x)',color='r')\n",
    "plt.legend(loc='lower right')\n",
    "plt.show() "
   ]
  }
